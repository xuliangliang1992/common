<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:paddingEnd="14dp"
        android:paddingStart="14dp">

        <ImageView
            android:id="@+id/tv_bottom"
            android:layout_width="208dp"
            android:layout_height="24dp"
            android:layout_marginBottom="23dp"
            android:layout_marginTop="17dp"
            android:src="@drawable/home_bottom"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:layout_width="26dp"
            android:layout_height="1dp"
            android:layout_marginEnd="14dp"
            android:background="#D4D8E0"
            app:layout_constraintBottom_toBottomOf="@+id/tv_bottom"
            app:layout_constraintEnd_toStartOf="@+id/tv_bottom"
            app:layout_constraintTop_toTopOf="@+id/tv_bottom" />

        <View
            android:layout_width="26dp"
            android:layout_height="1dp"
            android:layout_marginStart="14dp"
            android:background="#D4D8E0"
            app:layout_constraintBottom_toBottomOf="@+id/tv_bottom"
            app:layout_constraintStart_toEndOf="@+id/tv_bottom"
            app:layout_constraintTop_toTopOf="@+id/tv_bottom" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <data>

        <variable
            name="model"
            type="com.highlands.common.http.response.VideoBean" />
    </data>
</layout>